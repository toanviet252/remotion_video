{
	"compilerOptions": {
		"target": "ES2018",
		"module": "commonjs",
		"jsx": "react-jsx",
		"outDir": "./dist",
		"strict": true,
		"noEmit": true,
		"lib": [
			"es2015"
		],
		"baseUrl": ".",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"paths": {
			"@/*": [
				"src/*"
			],
		}
	}
}